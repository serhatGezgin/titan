module Deneme Nested Functions
   
package deneme.model{              
	entity Graph{                        
		var name String      
		var edges Edge[]    
		var vertices Vertex[] unique       
	}                          
	                     
	entity Edge{    
		var from Vertex opposite Graph.vertices
		var to Vertex   opposite Graph.vertices
		var weight Double
	}  
	    
	entity Vertex{
		var label String
	}
}